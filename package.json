{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format+lint": "prettier --write \"**/*.{ts,tsx,md}\" && turbo run lint",
    "commit": "commit",
    "prepare": "husky install"
  },
  "dependencies": {
    "husky": "^8.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@commitlint/prompt-cli": "^17.4.0",
    "eslint": "^8.39.0",
    "eslint-config-custom": "workspace:*",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.8",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "repositories"
}
